#include<stdio.h>
#include<stdlib.h>
#include "crossops.h"

void slotAdj(struct slot ** board){

	for(size_t i = 1; i<6;i++){

		for(size_t j = 1; j<6; j++){

			board[i][j].adj4 = 4;
		}
	}

	for(size_t i = 1;i < 6;i++){

		board[0][i].adj3 = 3;

		board[6][i].adj3 = 3;
	}
}
